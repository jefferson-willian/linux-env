[diff]
  tool = icdiff
[difftool]
  prompt = false
[difftool "icdiff"]
  cmd = icdiff -HN "$LOCAL" "$REMOTE"
[alias]
  a  = add
  c  = commit
  d  = diff
  l  = log
  s  = status
  dt = difftool
  ps = push
  pl = pull
  st-ls = !"git log | grep git-subtree-dir | tr -d ' ' | cut -d \":\" -f2 | sort | uniq"
[core]
  editor = vim
[credential]
  helper = store
